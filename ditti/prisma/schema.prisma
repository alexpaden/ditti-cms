generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model audience_trackers {
  id         BigInt   @id(map: "audience_tracker_pkey") @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  following  Json
  followers  Json
  fid        BigInt   @unique
}

model follow_trackers {
  id                Int       @id @default(autoincrement())
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  fid               Int?
  following_fids    Json?
  following_changes Json?
  follower_fids     Json?
  follower_changes  Json?
}

model follower_trackers {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  added      Json?
  removed    Json?
  fid        BigInt
}

model following_trackers {
  id         BigInt   @id @default(autoincrement())
  created_at DateTime @default(now()) @db.Timestamptz(6)
  added      Json
  removed    Json
  fid        BigInt
}

model profile_trackers {
  id              Int       @id @default(autoincrement())
  created_at      DateTime @db.Timestamptz(6)
  fid             Int
  username        String   @db.VarChar(75)
  display_name    String?   @db.VarChar(64)
  bio             String?
  following_count Int?
  follower_count  Int?
  pfp_url         String?
}
